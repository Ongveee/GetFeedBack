<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.0/css/all.min.css" integrity="sha512-xh6O/CkQoPOWDdYTDqeRdPCVd1SpvCA9XXcUnZS2FmJNp1coAFzvtCN9BmamE+4aHK8yyUHUSCcJHgXloTyT2A==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link href='~/centent/css/font-awesome.css' rel='stylesheet' type='text/css' />
    <link href='~/centent/css/elegantfont.css' rel='stylesheet' type='text/css' />
    <link rel="stylesheet" href="~/centent/Bootstrap/bootstrap-5.0.2-dist/css/bootstrap.min.css.map">
    <link rel="stylesheet" href="~/centent/css/style.css">
    <link rel="stylesheet" href="~/centent/css/size.css">
    <link rel="stylesheet" href="~/centent/scss/main.css">

    <title>Document</title>
</head>
<body>
    <div id="site-header">
        <header class="header">
            <div class="container-fluid">
                <div class="row">
                    <div class="header_content p-0">
                        <div class="header_site_info_parent">
                            <div class="header_site_info">
                                <div class="row">
                                    <div class="site_name col-md-3 col-lg-3 ">
                                        <a href="#">
                                            <img src="~/centent/images/AX-DMS1.png" />
                                        </a>
                                    </div>
                                    <div class="header_title col-md-6 col-lg-6">
                                        <h3>
                                            CỔNG DỊCH VỤ CÔNG TRỰC TUYẾN
                                        </h3>
                                    </div>
                                    <div class="drop_user col-md-3 col-lg-3">

                                        
                                        <a class="login btn btn-light ml-6" asp-action="Login" asp-controller="Account">Đăng nhập</a>
                                        <a class="login btn btn-light ml-6" asp-action="Register" asp-controller="Account">Đăng ký</a>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </header>
    </div>
    <div>
            @RenderBody()
    </div>
    <script src="~/centent/js/jquery-2.2.4.min.js"></script>
    <script src="~/centent/js/jquery.min.js"></script>
    <script src="~/centent/Bootstrap/bootstrap-5.0.2-dist/js/bootstrap.min.js"></script>
    <script src="~/centent/js/main.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>